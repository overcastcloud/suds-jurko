syntax: regexp

# 'setup.py' artifacts.
^build/
^dist/
\.egg$
\.egg-info/
\.gz$
\.zip$

# Temporary or cache files & folders created by Python (e.g. __pycache__
# folders) or our internal development tools.
^__
/__

# Precompiled Python modules outside a __pycache__ folder.
\.pyc$
\.pyo$

# Backup files.
\.bak$
